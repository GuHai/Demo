<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.ContentgroupDao" ><!-- 查询字段 --><sql id="columnList" >	c.id,	 c.contentCode, c.dictName, c.configureTime, c.sort, c.parentID, c.createTime, c.updateTime, c.version, c.isDeleted, c.createBy, c.updateBy</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  c.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and c.id = #{condition.id}	</if>	<if test="condition.contentCode != null" >and  c.contentCode = #{condition.contentCode}</if><if test="condition.dictName != null" >and  c.dictName = #{condition.dictName}</if><if test="condition.configureTime != null" >and  c.configureTime = #{condition.configureTime}</if><if test="condition.sort != null" >and  c.sort = #{condition.sort}</if><if test="condition.parentID != null" >and  c.parentID = #{condition.parentID}</if><if test="condition.createTime != null" >and  c.createTime = #{condition.createTime}</if><if test="condition.updateTime != null" >and  c.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  c.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  c.isDeleted = #{condition.isDeleted}</if><if test="condition.createBy != null" >and  c.createBy = #{condition.createBy}</if><if test="condition.updateBy != null" >and  c.updateBy = #{condition.updateBy}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and c.id = #{condition.id}	</if>													 			<if test="condition.contentCode != null" >and  UPPER(c.contentCode) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.contentCode}),'%'))</if><if test="condition.dictName != null" >and  UPPER(c.dictName) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.dictName}),'%'))</if><if test="condition.configureTime != null" >and  UPPER(c.configureTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.configureTime}),'%'))</if><if test="condition.sort != null" >and  UPPER(c.sort) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.sort}),'%'))</if><if test="condition.parentID != null" >and  UPPER(c.parentID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.parentID}),'%'))</if><if test="condition.createTime != null" >and  UPPER(c.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(c.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(c.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(c.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if><if test="condition.createBy != null" >and  UPPER(c.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(c.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into contentgroup (id,contentCode,dictName,configureTime,sort,parentID,createTime,updateTime,version,isDeleted,createBy,updateBy)  values(#{id,jdbcType=VARCHAR},#{contentCode},#{dictName},#{configureTime},#{sort},#{parentID},#{createTime},#{updateTime},#{version},#{isDeleted},#{createBy},#{updateBy})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update contentgroup set	<trim  suffixOverrides="," >	<if test="contentCode != null ">contentCode=#{contentCode},</if><if test="dictName != null ">dictName=#{dictName},</if><if test="configureTime != null ">configureTime=#{configureTime},</if><if test="sort != null ">sort=#{sort},</if><if test="parentID != null ">parentID=#{parentID},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="updateBy != null ">updateBy=#{updateBy},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from contentgroup where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Contentgroup"  parameterType="String">	select <include refid="columnList" /> 	from contentgroup  c	where  c.id = #{id}  and c.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from contentgroup  c	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Contentgroup" >	select 	<include refid="columnList"/>	from contentgroup c	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Contentgroup" >	select 	<include refid="columnList"/>	from contentgroup c	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Contentgroup" >	select	<include refid="columnList"/>	from contentgroup c	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Contentgroup" >	select	<include refid="columnList"/>	from contentgroup c	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select c.${idsColumn}	from contentgroup c    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select c.${idsColumn}	from contentgroup c    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Contentgroup" >     select    <include refid="columnList"/>     from contentgroup c    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>