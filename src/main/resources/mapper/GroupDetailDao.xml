<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.GroupDetailDao" ><!-- 查询字段 --><sql id="columnList" >	c.id,	 c.name, c.positionID, c.groupID, c.createBy, c.createTime, c.updateBy, c.updateTime, c.version, c.isDeleted</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  c.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and c.id = #{condition.id}	</if>	<if test="condition.name != null" >and  c.name = #{condition.name}</if><if test="condition.positionID != null" >and  c.positionID = #{condition.positionID}</if><if test="condition.groupID != null" >and  c.groupID = #{condition.groupID}</if><if test="condition.createBy != null" >and  c.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  c.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  c.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  c.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  c.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  c.isDeleted = #{condition.isDeleted}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and c.id = #{condition.id}	</if>													 			<if test="condition.name != null" >and  UPPER(c.name) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.name}),'%'))</if><if test="condition.positionID != null" >and  UPPER(c.positionID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.positionID}),'%'))</if><if test="condition.groupID != null" >and  UPPER(c.groupID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.groupID}),'%'))</if><if test="condition.createBy != null" >and  UPPER(c.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(c.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(c.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(c.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(c.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(c.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into chatgroupdetail (id,name,positionID,groupID,createBy,createTime,updateBy,updateTime,version,isDeleted)  values(#{id,jdbcType=VARCHAR},#{name},#{positionID},#{groupID},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update chatgroupdetail set	<trim  suffixOverrides="," >	<if test="name != null ">name=#{name},</if><if test="positionID != null ">positionID=#{positionID},</if><if test="groupID != null ">groupID=#{groupID},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from chatgroupdetail where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="GroupDetail"  parameterType="String">	select <include refid="columnList" /> 	from chatgroupdetail  c	where  c.id = #{id}  and c.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from chatgroupdetail  c	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="GroupDetail" >	select 	<include refid="columnList"/>	from chatgroupdetail c	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="GroupDetail" >	select 	<include refid="columnList"/>	from chatgroupdetail c	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="GroupDetail" >	select	<include refid="columnList"/>	from chatgroupdetail c	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="GroupDetail" >	select	<include refid="columnList"/>	from chatgroupdetail c	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select c.${idsColumn}	from chatgroupdetail c    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select c.${idsColumn}	from chatgroupdetail c    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="GroupDetail" >     select    <include refid="columnList"/>     from chatgroupdetail c    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>