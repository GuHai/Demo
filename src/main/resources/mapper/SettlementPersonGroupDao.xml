<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.SettlementPersonGroupDao" ><!-- 查询字段 --><sql id="columnList" >	s.id,	 s.applyId, s.totalMoney, s.size, s.isPay, s.createBy, s.createTime, s.updateBy, s.updateTime, s.version, s.isDeleted, s.positionID</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  s.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and s.id = #{condition.id}	</if>	<if test="condition.applyId != null" >and  s.applyId = #{condition.applyId}</if><if test="condition.totalMoney != null" >and  s.totalMoney = #{condition.totalMoney}</if><if test="condition.sizes != null" >and  s.size = #{condition.sizes}</if><if test="condition.isPay != null" >and  s.isPay = #{condition.isPay}</if><if test="condition.createBy != null" >and  s.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  s.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  s.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  s.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  s.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  s.isDeleted = #{condition.isDeleted}</if><if test="condition.positionID != null" >and  s.positionID = #{condition.positionID}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and s.id = #{condition.id}	</if>													 			<if test="condition.applyId != null" >and  UPPER(s.applyId) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.applyId}),'%'))</if><if test="condition.totalMoney != null" >and  UPPER(s.totalMoney) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.totalMoney}),'%'))</if><if test="condition.size != null" >and  UPPER(s.size) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.size}),'%'))</if><if test="condition.isPay != null" >and  UPPER(s.isPay) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isPay}),'%'))</if><if test="condition.createBy != null" >and  UPPER(s.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(s.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(s.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(s.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(s.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(s.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if><if test="condition.positionID != null" >and  UPPER(s.positionID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.positionID}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into settlementpersongroup (id,applyId,totalMoney,size,isPay,createBy,createTime,updateBy,updateTime,version,isDeleted,positionID)  values(#{id,jdbcType=VARCHAR},#{applyId},#{totalMoney},#{size},#{isPay},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted},#{positionID})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update settlementpersongroup set	<trim  suffixOverrides="," >	<if test="applyId != null ">applyId=#{applyId},</if><if test="totalMoney != null ">totalMoney=#{totalMoney},</if><if test="size != null ">size=#{size},</if><if test="isPay != null ">isPay=#{isPay},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if><if test="positionID != null ">positionID=#{positionID},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from settlementpersongroup where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="SettlementPersonGroup"  parameterType="String">	select <include refid="columnList" /> 	from settlementpersongroup  s	where  s.id = #{id}  and s.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from settlementpersongroup  s	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="SettlementPersonGroup" >	select 	<include refid="columnList"/>	from settlementpersongroup s	<include refid="whereClause"/>	<if test="condition.signtype!='all'">		AND  s.isPay = #{condition.signtype}	</if>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="SettlementPersonGroup" >	select 	<include refid="columnList"/>	from settlementpersongroup s	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="SettlementPersonGroup" >	select	<include refid="columnList"/>	from settlementpersongroup s	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="SettlementPersonGroup" >	select	<include refid="columnList"/>	from settlementpersongroup s	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select s.${idsColumn}	from settlementpersongroup s    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select s.${idsColumn}	from settlementpersongroup s    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="SettlementPersonGroup" >     select    <include refid="columnList"/>     from settlementpersongroup s    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select>	<!--级联查询-->	<sql id="salarydetailList">		s.id as 'id',		s.version as 'version',s.applyId as 'applyId',s.totalMoney as 'totalMoney',s.size as 'size',s.isPay as 'isPay',s.positionID as 'positionID',s.createBy as 'createBy',s.updateTime as 'updateTime',		wxorder.id as 'wxorder.id',		wxorder.version as 'wxorder.version',wxorder.name as 'wxorder.name',wxorder.description as 'wxorder.description',wxorder.type as 'wxorder.type',wxorder.code as 'wxorder.code',wxorder.status as 'wxorder.status',wxorder.userID as 'wxorder.userID',wxorder.fee as 'wxorder.fee',wxorder.prepayID as 'wxorder.prepayID',wxorder.refID as 'wxorder.refID'	</sql>	<!--salaryWxorder映射-->	<resultMap id="salaryWxorderMap" type="Wxorder">		<id property="id" column="wxorder.id" />		<result property="version" column="wxorder.version" />		<result property="name" column="wxorder.name" />		<result property="description" column="wxorder.description" />		<result property="type" column="wxorder.type" />		<result property="code" column="wxorder.code" />		<result property="status" column="wxorder.status" />		<result property="userID" column="wxorder.userID" />		<result property="fee" column="wxorder.fee" />		<result property="prepayID" column="wxorder.prepayID" />		<result property="refID" column="wxorder.refID" />	</resultMap>	<!--salarySettlementPersonGroup映射-->	<resultMap id="salarySettlementPersonGroupMap" type="SettlementPersonGroup">		<id property="id" column="id" />		<result property="version" column="version" />		<result property="applyId" column="applyId" />		<result property="totalMoney" column="totalMoney" />		<result property="size" column="size" />		<result property="isPay" column="isPay" />		<result property="positionID" column="positionID" />		<result property="createBy" column="createBy" />		<result property="updateTime" column="updateTime" />		<association property="wxorder"  javaType="Wxorder" resultMap="salaryWxorderMap"/>	</resultMap>	<!--对象的多级映射 mapList mapPage mapOne-->	<select id="salarymapOne" resultMap="salarySettlementPersonGroupMap" >		select		<include refid="salarydetailList"/>		from  settlementpersongroup s		left join wxorder wxorder on s.id = wxorder.refID		<include refid="whereClause"/>		<if test="orderByClause!=null">${orderByClause}</if>		limit 0,1	</select></mapper>