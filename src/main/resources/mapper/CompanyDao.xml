<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.CompanyDao" ><!-- 查询字段 --><sql id="columnList" >	pc.id,	 pc.company, pc.addrID, pc.createBy, pc.createTime, pc.updateBy, pc.updateTime, pc.version, pc.isDeleted</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  pc.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and pc.id = #{condition.id}	</if>	<if test="condition.company != null" >and  pc.company = #{condition.company}</if><if test="condition.addrID != null" >and  pc.addrID = #{condition.addrID}</if><if test="condition.createBy != null" >and  pc.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  pc.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  pc.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  pc.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  pc.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  pc.isDeleted = #{condition.isDeleted}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and pc.id = #{condition.id}	</if>													 			<if test="condition.company != null" >and  UPPER(pc.company) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.company}),'%'))</if><if test="condition.addrID != null" >and  UPPER(pc.addrID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.addrID}),'%'))</if><if test="condition.createBy != null" >and  UPPER(pc.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(pc.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(pc.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(pc.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(pc.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(pc.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into post_company (id,company,addrID,createBy,createTime,updateBy,updateTime,version,isDeleted)  values(#{id,jdbcType=VARCHAR},#{company},#{addrID},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update post_company set	<trim  suffixOverrides="," >	<if test="company != null ">company=#{company},</if><if test="addrID != null ">addrID=#{addrID},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from post_company where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Company"  parameterType="String">	select <include refid="columnList" /> 	from post_company  pc	where  pc.id = #{id}  and pc.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from post_company  pc	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Company" >	select 	<include refid="columnList"/>	from post_company pc	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Company" >	select 	<include refid="columnList"/>	from post_company pc	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Company" >	select	<include refid="columnList"/>	from post_company pc	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Company" >	select	<include refid="columnList"/>	from post_company pc	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select pc.${idsColumn}	from post_company pc    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select pc.${idsColumn}	from post_company pc    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Company" >     select    <include refid="columnList"/>     from post_company pc    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>