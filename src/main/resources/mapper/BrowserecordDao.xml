<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.BrowserecordDao" ><!-- 查询字段 --><sql id="columnList" >	b.id,	 b.userID, b.browseType, b.browseTime, b.sleepTime, b.browseAddress, b.createTime, b.updateTime, b.version, b.isDeleted, b.createBy, b.updateBy, b.browseID</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  b.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and b.id = #{condition.id}	</if>	<if test="condition.userID != null" >and  b.userID = #{condition.userID}</if><if test="condition.browseType != null" >and  b.browseType = #{condition.browseType}</if><if test="condition.browseTime != null" >and  b.browseTime = #{condition.browseTime}</if><if test="condition.sleepTime != null" >and  b.sleepTime = #{condition.sleepTime}</if><if test="condition.browseAddress != null" >and  b.browseAddress = #{condition.browseAddress}</if><if test="condition.createTime != null" >and  b.createTime = #{condition.createTime}</if><if test="condition.updateTime != null" >and  b.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  b.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  b.isDeleted = #{condition.isDeleted}</if><if test="condition.createBy != null" >and  b.createBy = #{condition.createBy}</if><if test="condition.updateBy != null" >and  b.updateBy = #{condition.updateBy}</if><if test="condition.browseID != null" >and  b.browseID = #{condition.browseID}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and b.id = #{condition.id}	</if>													 			<if test="condition.userID != null" >and  UPPER(b.userID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.userID}),'%'))</if><if test="condition.browseType != null" >and  UPPER(b.browseType) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.browseType}),'%'))</if><if test="condition.browseTime != null" >and  UPPER(b.browseTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.browseTime}),'%'))</if><if test="condition.sleepTime != null" >and  UPPER(b.sleepTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.sleepTime}),'%'))</if><if test="condition.browseAddress != null" >and  UPPER(b.browseAddress) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.browseAddress}),'%'))</if><if test="condition.createTime != null" >and  UPPER(b.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(b.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(b.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(b.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if><if test="condition.createBy != null" >and  UPPER(b.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(b.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.browseID != null" >and  UPPER(b.browseID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.browseID}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into browserecord (id,userID,browseType,browseTime,sleepTime,browseAddress,createTime,updateTime,version,isDeleted,createBy,updateBy,browseID)  values(#{id,jdbcType=VARCHAR},#{userID},#{browseType},#{browseTime},#{sleepTime},#{browseAddress},#{createTime},#{updateTime},#{version},#{isDeleted},#{createBy},#{updateBy},#{browseID})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update browserecord set	<trim  suffixOverrides="," >	<if test="userID != null ">userID=#{userID},</if><if test="browseType != null ">browseType=#{browseType},</if><if test="browseTime != null ">browseTime=#{browseTime},</if><if test="sleepTime != null ">sleepTime=#{sleepTime},</if><if test="browseAddress != null ">browseAddress=#{browseAddress},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="browseID != null ">browseID=#{browseID},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from browserecord where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Browserecord"  parameterType="String">	select <include refid="columnList" /> 	from browserecord  b	where  b.id = #{id}  and b.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from browserecord  b	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Browserecord" >	select 	<include refid="columnList"/>	from browserecord b	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Browserecord" >	select 	<include refid="columnList"/>	from browserecord b	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Browserecord" >	select	<include refid="columnList"/>	from browserecord b	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Browserecord" >	select	<include refid="columnList"/>	from browserecord b	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select b.${idsColumn}	from browserecord b    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select b.${idsColumn}	from browserecord b    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Browserecord" >     select    <include refid="columnList"/>     from browserecord b    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>