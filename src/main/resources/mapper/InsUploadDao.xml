<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.InsUploadDao" ><!-- 查询字段 --><sql id="columnList" >	i.id,	 i.name, i.status, i.type, i.batch, i.createBy, i.createTime, i.updateBy, i.updateTime, i.version, i.isDeleted</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  i.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and i.id = #{condition.id}	</if>	<if test="condition.name != null" >and  i.name = #{condition.name}</if><if test="condition.status != null" >and  i.status = #{condition.status}</if><if test="condition.type != null" >and  i.type = #{condition.type}</if><if test="condition.batch != null" >and  i.batch = #{condition.batch}</if><if test="condition.createBy != null" >and  i.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  i.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  i.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  i.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  i.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  i.isDeleted = #{condition.isDeleted}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and i.id = #{condition.id}	</if>													 			<if test="condition.name != null" >and  UPPER(i.name) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.name}),'%'))</if><if test="condition.status != null" >and  UPPER(i.status) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.status}),'%'))</if><if test="condition.type != null" >and  UPPER(i.type) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.type}),'%'))</if><if test="condition.batch != null" >and  UPPER(i.batch) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.batch}),'%'))</if><if test="condition.createBy != null" >and  UPPER(i.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(i.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(i.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(i.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(i.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(i.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into insupload (id,name,status,type,batch,createBy,createTime,updateBy,updateTime,version,isDeleted)  values(#{id,jdbcType=VARCHAR},#{name},#{status},#{type},#{batch},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update insupload set	<trim  suffixOverrides="," >	<if test="name != null ">name=#{name},</if><if test="status != null ">status=#{status},</if><if test="type != null ">type=#{type},</if><if test="batch != null ">batch=#{batch},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from insupload where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="InsUpload"  parameterType="String">	select <include refid="columnList" /> 	from insupload  i	where  i.id = #{id}  and i.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from insupload  i	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="InsUpload" >	select 	<include refid="columnList"/>	from insupload i	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="InsUpload" >	select 	<include refid="columnList"/>	from insupload i	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="InsUpload" >	select	<include refid="columnList"/>	from insupload i	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="InsUpload" >	select	<include refid="columnList"/>	from insupload i	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select i.${idsColumn}	from insupload i    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select i.${idsColumn}	from insupload i    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="InsUpload" >     select    <include refid="columnList"/>     from insupload i    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>