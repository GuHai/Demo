<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.LinkDao" ><!-- 查询字段 --><sql id="columnList" >	l.id,l.content,l.init,	 l.title, l.link, l.createBy, l.createTime, l.updateBy, l.updateTime, l.version, l.isDeleted</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  l.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and l.id = #{condition.id}	</if>	<if test="condition.title != null" >and  l.title = #{condition.title}</if><if test="condition.link != null" >and  l.link = #{condition.link}</if><if test="condition.createBy != null" >and  l.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  l.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  l.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  l.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  l.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  l.isDeleted = #{condition.isDeleted}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and l.id = #{condition.id}	</if>													 			<if test="condition.title != null" >and  UPPER(l.title) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.title}),'%'))</if><if test="condition.link != null" >and  UPPER(l.link) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.link}),'%'))</if><if test="condition.createBy != null" >and  UPPER(l.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(l.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(l.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(l.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(l.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(l.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into link (id,title,link,createBy,createTime,updateBy,updateTime,version,isDeleted)  values(#{id,jdbcType=VARCHAR},#{title},#{link},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update link set	<trim  suffixOverrides="," >	<if test="title != null ">title=#{title},</if><if test="link != null ">link=#{link},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if>		<if test="init != null ">init=#{init},</if>		<if test="content != null ">content=#{content},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from link where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Link"  parameterType="String">	select <include refid="columnList" /> 	from link  l	where  l.id = #{id}  and l.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from link  l	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Link" >	select 	<include refid="columnList"/>	from link l	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Link" >	select 	<include refid="columnList"/>	from link l	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Link" >	select	<include refid="columnList"/>	from link l	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Link" >	select	<include refid="columnList"/>	from link l	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select l.${idsColumn}	from link l    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select l.${idsColumn}	from link l    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Link" >     select    <include refid="columnList"/>     from link l    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>