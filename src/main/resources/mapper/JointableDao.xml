<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.JointableDao" ><!-- 查询字段 --><sql id="columnList" >	j.id,	 j.name, j.phoneNumber, j.joinType, j.region, j.explain, j.createTime, j.updateTime, j.version, j.isDeleted, j.createBy, j.updateBy</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  j.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and j.id = #{condition.id}	</if>	<if test="condition.name != null" >and  j.name = #{condition.name}</if><if test="condition.phoneNumber != null" >and  j.phoneNumber = #{condition.phoneNumber}</if><if test="condition.joinType != null" >and  j.joinType = #{condition.joinType}</if><if test="condition.region != null" >and  j.region = #{condition.region}</if><if test="condition.explain != null" >and  j.explain = #{condition.explain}</if><if test="condition.createTime != null" >and  j.createTime = #{condition.createTime}</if><if test="condition.updateTime != null" >and  j.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  j.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  j.isDeleted = #{condition.isDeleted}</if><if test="condition.createBy != null" >and  j.createBy = #{condition.createBy}</if><if test="condition.updateBy != null" >and  j.updateBy = #{condition.updateBy}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and j.id = #{condition.id}	</if>													 			<if test="condition.name != null" >and  UPPER(j.name) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.name}),'%'))</if><if test="condition.phoneNumber != null" >and  UPPER(j.phoneNumber) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.phoneNumber}),'%'))</if><if test="condition.joinType != null" >and  UPPER(j.joinType) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.joinType}),'%'))</if><if test="condition.region != null" >and  UPPER(j.region) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.region}),'%'))</if><if test="condition.explain != null" >and  UPPER(j.explain) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.explain}),'%'))</if><if test="condition.createTime != null" >and  UPPER(j.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(j.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(j.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(j.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if><if test="condition.createBy != null" >and  UPPER(j.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(j.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into jointable (id,`name`,phoneNumber,joinType,region,`explain`,createTime,updateTime,`version`,isDeleted,createBy,updateBy)  values(#{id,jdbcType=VARCHAR},#{name},#{phoneNumber},#{joinType},#{region},#{explain},#{createTime},#{updateTime},#{version},#{isDeleted},#{createBy},#{updateBy})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update jointable set	<trim  suffixOverrides="," >	<if test="name != null ">name=#{name},</if><if test="phoneNumber != null ">phoneNumber=#{phoneNumber},</if><if test="joinType != null ">joinType=#{joinType},</if><if test="region != null ">region=#{region},</if><if test="explain != null ">explain=#{explain},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="updateBy != null ">updateBy=#{updateBy},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from jointable where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Jointable"  parameterType="String">	select <include refid="columnList" /> 	from jointable  j	where  j.id = #{id}  and j.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from jointable  j	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Jointable" >	select 	<include refid="columnList"/>	from jointable j	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Jointable" >	select 	<include refid="columnList"/>	from jointable j	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Jointable" >	select	<include refid="columnList"/>	from jointable j	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Jointable" >	select	<include refid="columnList"/>	from jointable j	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select j.${idsColumn}	from jointable j    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select j.${idsColumn}	from jointable j    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Jointable" >     select    <include refid="columnList"/>     from jointable j    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>