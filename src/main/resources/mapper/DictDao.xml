<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.DictDao" ><!-- 查询字段 --><sql id="columnList" >	d.id,	 d.dictCode, d.dictContent, d.title, d.sort, d.parentID, d.createTime, d.updateTime, d.version, d.isDeleted, d.createBy, d.updateBy</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  d.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and d.id = #{condition.id}	</if>	<if test="condition.dictCode != null" >and  d.dictCode = #{condition.dictCode}</if><if test="condition.dictContent != null" >and  d.dictContent = #{condition.dictContent}</if><if test="condition.title != null" >and  d.title = #{condition.title}</if><if test="condition.sort != null" >and  d.sort = #{condition.sort}</if><if test="condition.parentID != null" >and  d.parentID = #{condition.parentID}</if><if test="condition.createTime != null" >and  d.createTime = #{condition.createTime}</if><if test="condition.updateTime != null" >and  d.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  d.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  d.isDeleted = #{condition.isDeleted}</if><if test="condition.createBy != null" >and  d.createBy = #{condition.createBy}</if><if test="condition.updateBy != null" >and  d.updateBy = #{condition.updateBy}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and d.id = #{condition.id}	</if>													 			<if test="condition.dictCode != null" >and  UPPER(d.dictCode) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.dictCode}),'%'))</if><if test="condition.dictContent != null" >and  UPPER(d.dictContent) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.dictContent}),'%'))</if><if test="condition.title != null" >and  UPPER(d.title) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.title}),'%'))</if><if test="condition.sort != null" >and  UPPER(d.sort) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.sort}),'%'))</if><if test="condition.parentID != null" >and  UPPER(d.parentID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.parentID}),'%'))</if><if test="condition.createTime != null" >and  UPPER(d.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(d.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(d.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(d.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if><if test="condition.createBy != null" >and  UPPER(d.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(d.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into dict (id,dictCode,dictContent,title,sort,parentID,createTime,updateTime,version,isDeleted,createBy,updateBy)  values(#{id,jdbcType=VARCHAR},#{dictCode},#{dictContent},#{title},#{sort},#{parentID},#{createTime},#{updateTime},#{version},#{isDeleted},#{createBy},#{updateBy})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update dict set	<trim  suffixOverrides="," >	<if test="dictCode != null ">dictCode=#{dictCode},</if><if test="dictContent != null ">dictContent=#{dictContent},</if><if test="title != null ">title=#{title},</if><if test="sort != null ">sort=#{sort},</if><if test="parentID != null ">parentID=#{parentID},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="updateBy != null ">updateBy=#{updateBy},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from dict where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Dict"  parameterType="String">	select <include refid="columnList" /> 	from dict  d	where  d.id = #{id}  and d.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from dict  d	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Dict" >	select 	<include refid="columnList"/>	from dict d	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Dict" >	select 	<include refid="columnList"/>	from dict d	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Dict" >	select	<include refid="columnList"/>	from dict d	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Dict" >	select	<include refid="columnList"/>	from dict d	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select d.${idsColumn}	from dict d    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select d.${idsColumn}	from dict d    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Dict" >     select    <include refid="columnList"/>     from dict d    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>