<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.MycardDao" ><!-- 查询字段 --><sql id="columnList" >	m.id,m.code,m.start,m.end,	 m.cardId, m.flag, m.createBy, m.createTime, m.updateBy, m.updateTime, m.version, m.isDeleted</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  m.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and m.id = #{condition.id}	</if>	<if test="condition.cardId != null" >and  m.cardId = #{condition.cardId}</if><if test="condition.flag != null" >and  m.flag = #{condition.flag}</if><if test="condition.createBy != null" >and  m.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  m.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  m.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  m.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  m.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  m.isDeleted = #{condition.isDeleted}</if>	<if test="condition.code != null" >and  m.code = #{condition.code}</if>	<if test="condition.start != null" >and  m.start = #{condition.start}</if>	<if test="condition.end != null" >and  m.end = #{condition.end}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and m.id = #{condition.id}	</if>													 			<if test="condition.cardId != null" >and  UPPER(m.cardId) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.cardId}),'%'))</if><if test="condition.flag != null" >and  UPPER(m.flag) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.flag}),'%'))</if><if test="condition.createBy != null" >and  UPPER(m.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(m.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(m.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(m.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(m.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(m.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into mycard (id,start,end,cardId,code,flag,createBy,createTime,updateBy,updateTime,version,isDeleted)  values(#{id,jdbcType=VARCHAR},#{start},#{end},#{cardId},#{code},#{flag},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update mycard set	<trim  suffixOverrides="," >	<if test="cardId != null ">cardId=#{cardId},</if><if test="flag != null ">flag=#{flag},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if>		<if test="code != null ">code=#{code},</if>		<if test="start != null ">start=#{start},</if>		<if test="end != null ">end=#{end},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from mycard where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Mycard"  parameterType="String">	select <include refid="columnList" /> 	from mycard  m	where  m.id = #{id}  and m.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from mycard  m	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Mycard" >	select 	<include refid="columnList"/>	from mycard m	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Mycard" >	select 	<include refid="columnList"/>	from mycard m	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Mycard" >	select	<include refid="columnList"/>,c.price,c.name,c.imgpath	from mycard m left join cardticket c on m.cardId = c.id	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Mycard" >	select	<include refid="columnList"/>	from mycard m	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select m.${idsColumn}	from mycard m    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select m.${idsColumn}	from mycard m    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Mycard" >     select    <include refid="columnList"/>     from mycard m    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>