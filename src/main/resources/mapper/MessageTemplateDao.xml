<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.MessageTemplateDao" ><!-- 查询字段 --><sql id="columnList" >	m.id,	 m.templateID, m.title, m.type, m.createBy, m.createTime, m.updateBy, m.updateTime, m.version, m.isDeleted</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  m.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and m.id = #{condition.id}	</if>	<if test="condition.templateID != null" >and  m.templateID = #{condition.templateID}</if><if test="condition.title != null" >and  m.title = #{condition.title}</if><if test="condition.type != null" >and  m.type = #{condition.type}</if><if test="condition.createBy != null" >and  m.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  m.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  m.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  m.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  m.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  m.isDeleted = #{condition.isDeleted}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and m.id = #{condition.id}	</if>													 			<if test="condition.templateID != null" >and  UPPER(m.templateID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.templateID}),'%'))</if><if test="condition.title != null" >and  UPPER(m.title) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.title}),'%'))</if><if test="condition.type != null" >and  UPPER(m.type) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.type}),'%'))</if><if test="condition.createBy != null" >and  UPPER(m.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(m.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(m.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(m.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(m.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(m.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into messagetemplate (id,templateID,title,type,createBy,createTime,updateBy,updateTime,version,isDeleted)  values(#{id,jdbcType=VARCHAR},#{templateID},#{title},#{type},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update messagetemplate set	<trim  suffixOverrides="," >	<if test="templateID != null ">templateID=#{templateID},</if><if test="title != null ">title=#{title},</if><if test="type != null ">type=#{type},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from messagetemplate where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="MessageTemplate"  parameterType="String">	select <include refid="columnList" /> 	from messagetemplate  m	where  m.id = #{id}  and m.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from messagetemplate  m	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="MessageTemplate" >	select 	<include refid="columnList"/>	from messagetemplate m	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="MessageTemplate" >	select 	<include refid="columnList"/>	from messagetemplate m	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="MessageTemplate" >	select	<include refid="columnList"/>	from messagetemplate m	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="MessageTemplate" >	select	<include refid="columnList"/>	from messagetemplate m	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select m.${idsColumn}	from messagetemplate m    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select m.${idsColumn}	from messagetemplate m    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="MessageTemplate" >     select    <include refid="columnList"/>     from messagetemplate m    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>