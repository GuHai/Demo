<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.PartnerGzhDao" ><!-- 查询字段 --><sql id="columnList" >	p.id,	 p.openID, p.code, p.createBy, p.createTime, p.updateBy, p.updateTime, p.version, p.isDeleted,p.type</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  p.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and p.id = #{condition.id}	</if>	<if test="condition.openID != null" >and  p.openID = #{condition.openID}</if><if test="condition.code != null" >and  p.code = #{condition.code}</if><if test="condition.createBy != null" >and  p.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  p.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  p.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  p.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  p.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  p.isDeleted = #{condition.isDeleted}</if>	<if test="condition.isPartner != null " >and  p.type is null </if>	<if test="condition.type != null" >and  p.type = #{condition.type} </if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and p.id = #{condition.id}	</if>													 			<if test="condition.openID != null" >and  UPPER(p.openID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.openID}),'%'))</if><if test="condition.code != null" >and  UPPER(p.code) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.code}),'%'))</if><if test="condition.createBy != null" >and  UPPER(p.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(p.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(p.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(p.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(p.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(p.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into partnergzh (id,type,openID,code,createBy,createTime,updateBy,updateTime,version,isDeleted)  values(#{id,jdbcType=VARCHAR},#{type},#{openID},#{code},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update partnergzh set	<trim  suffixOverrides="," >	<if test="openID != null ">openID=#{openID},</if><if test="code != null ">code=#{code},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if><if test="type != null ">type=#{type},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from partnergzh where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="PartnerGzh"  parameterType="String">	select <include refid="columnList" /> 	from partnergzh  p	where  p.id = #{id}  and p.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from partnergzh  p	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="PartnerGzh" >	select 	<include refid="columnList"/>	from partnergzh p	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="PartnerGzh" >	select 	<include refid="columnList"/>	from partnergzh p	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="PartnerGzh" >	select	<include refid="columnList"/>	from partnergzh p	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="PartnerGzh" >	select	<include refid="columnList"/>	from partnergzh p	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select p.${idsColumn}	from partnergzh p    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select p.${idsColumn}	from partnergzh p    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="PartnerGzh" >     select    <include refid="columnList"/>     from partnergzh p    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select>	<!--级联查询-->	<sql id="pudetailList">		p.id as 'id',		p.version as 'version',p.openID as 'openID',p.code as 'code',p.type as 'type',p.updateTime as 'updateTime',		partnerUser.id as 'partnerUser.id',		partnerUser.version as 'partnerUser.version',partnerUser.userID as 'partnerUser.userID',partnerUser.partID as 'partnerUser.partID',partnerUser.shareID as 'partnerUser.shareID',partnerUser.status as 'partnerUser.status',partnerUser.code as 'partnerUser.code',partnerUser.upgradePartID as 'partnerUser.upgradePartID',		user.id as 'partnerUser.user.id',		user.version as 'partnerUser.user.version',user.accountNo as 'partnerUser.user.accountNo',user.password as 'partnerUser.user.password',user.weChatNo as 'partnerUser.user.weChatNo',user.nickName as 'partnerUser.user.nickName',user.sex as 'partnerUser.user.sex',user.phoneNumber as 'partnerUser.user.phoneNumber',user.lastLoginTime as 'partnerUser.user.lastLoginTime',user.loginTime as 'partnerUser.user.loginTime',user.status as 'partnerUser.user.status',user.realName as 'partnerUser.user.realName',user.IDCard as 'partnerUser.user.IDCard',user.infoHeadImg as 'partnerUser.user.infoHeadImg',user.locked as 'partnerUser.user.locked',user.userSig as 'partnerUser.user.userSig',user.birthday as 'partnerUser.user.birthday',user.pinyin as 'partnerUser.user.pinyin'	</sql>	<!--puUser映射-->	<resultMap id="puUserMap" type="User">		<id property="id" column="partnerUser.user.id" />		<result property="version" column="partnerUser.user.version" />		<result property="accountNo" column="partnerUser.user.accountNo" />		<result property="password" column="partnerUser.user.password" />		<result property="weChatNo" column="partnerUser.user.weChatNo" />		<result property="nickName" column="partnerUser.user.nickName" />		<result property="sex" column="partnerUser.user.sex" />		<result property="phoneNumber" column="partnerUser.user.phoneNumber" />		<result property="lastLoginTime" column="partnerUser.user.lastLoginTime" />		<result property="loginTime" column="partnerUser.user.loginTime" />		<result property="status" column="partnerUser.user.status" />		<result property="realName" column="partnerUser.user.realName" />		<result property="IDCard" column="partnerUser.user.IDCard" />		<result property="infoHeadImg" column="partnerUser.user.infoHeadImg" />		<result property="locked" column="partnerUser.user.locked" />		<result property="userSig" column="partnerUser.user.userSig" />		<result property="birthday" column="partnerUser.user.birthday" />		<result property="pinyin" column="partnerUser.user.pinyin" />		<result property="imgPath" column="partnerUser.user.imgPath" />	</resultMap>	<!--puPartnerUser映射-->	<resultMap id="puPartnerUserMap" type="PartnerUser">		<id property="id" column="partnerUser.id" />		<result property="version" column="partnerUser.version" />		<result property="userID" column="partnerUser.userID" />		<result property="partID" column="partnerUser.partID" />		<result property="shareID" column="partnerUser.shareID" />		<result property="status" column="partnerUser.status" />		<result property="code" column="partnerUser.code" />		<result property="upgradePartID" column="partnerUser.upgradePartID" />		<association property="user"  javaType="User" resultMap="puUserMap"/>	</resultMap>	<!--puPartnerGzh映射-->	<resultMap id="puPartnerGzhMap" type="PartnerGzh">		<id property="id" column="id" />		<result property="version" column="version" />		<result property="openID" column="openID" />		<result property="code" column="code" />		<result property="type" column="type" />		<result property="updateTime" column="updateTime" />		<association property="partnerUser"  javaType="PartnerUser" resultMap="puPartnerUserMap"/>	</resultMap>	<!--对象的多级映射 mapList mapPage mapOne-->	<select id="pumapOne" resultMap="puPartnerGzhMap" >		select		<include refid="pudetailList"/>,wx.headimgurl as 'partnerUser.user.imgPath'		from  partnergzh p		left join partneruser partnerUser on p.code = partnerUser.code		left join shiro_user user on partnerUser.userID = user.id		left join weixin wx on wx.userID = user.id		<include refid="whereClause"/>		<if test="orderByClause!=null">${orderByClause}</if>	</select></mapper>