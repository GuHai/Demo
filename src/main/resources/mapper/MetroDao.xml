<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.MetroDao" ><!-- 查询字段 --><sql id="columnList" >	m.id,	 m.name, m.cityID, m.num, m.createBy, m.createTime, m.updateBy, m.updateTime, m.version, m.isDeleted</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  m.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and m.id = #{condition.id}	</if>	<if test="condition.name != null" >and  m.name = #{condition.name}</if><if test="condition.cityID != null" >and  m.cityID = #{condition.cityID}</if><if test="condition.num != null" >and  m.num = #{condition.num}</if><if test="condition.createBy != null" >and  m.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  m.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  m.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  m.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  m.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  m.isDeleted = #{condition.isDeleted}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and m.id = #{condition.id}	</if>													 			<if test="condition.name != null" >and  UPPER(m.name) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.name}),'%'))</if><if test="condition.cityID != null" >and  UPPER(m.cityID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.cityID}),'%'))</if><if test="condition.num != null" >and  UPPER(m.num) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.num}),'%'))</if><if test="condition.createBy != null" >and  UPPER(m.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(m.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(m.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(m.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(m.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(m.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into metro (id,name,cityID,num,createBy,createTime,updateBy,updateTime,version,isDeleted)  values(#{id,jdbcType=VARCHAR},#{name},#{cityID},#{num},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update metro set	<trim  suffixOverrides="," >	<if test="name != null ">name=#{name},</if><if test="cityID != null ">cityID=#{cityID},</if><if test="num != null ">num=#{num},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from metro where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Metro"  parameterType="String">	select <include refid="columnList" /> 	from metro  m	where  m.id = #{id}  and m.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from metro  m	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Metro" >	select 	<include refid="columnList"/>	from metro m	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Metro" >	select 	<include refid="columnList"/>	from metro m	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Metro" >	select	<include refid="columnList"/>	from metro m	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Metro" >	select	<include refid="columnList"/>	from metro m	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select m.${idsColumn}	from metro m    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select m.${idsColumn}	from metro m    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Metro" >     select    <include refid="columnList"/>     from metro m    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select>	<select id="getMyCityMetro"  resultType="Metro" parameterType="Object">		select <include refid="columnList"/>from metro m where m.cityID = (select case when  c2.cityType=3 then c2.parentID else c2.id end  from city c2 where c2.id = #{condition.cityID})	</select></mapper>