<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.RefundDao" ><!-- 查询字段 --><sql id="columnList" >	r.id,	 r.orderID, r.refundFee, r.reason, r.createBy, r.createTime, r.updateBy, r.updateTime, r.version, r.isDeleted, r.code</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  r.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and r.id = #{condition.id}	</if>	<if test="condition.orderID != null" >and  r.orderID = #{condition.orderID}</if><if test="condition.refundFee != null" >and  r.refundFee = #{condition.refundFee}</if><if test="condition.reason != null" >and  r.reason = #{condition.reason}</if><if test="condition.createBy != null" >and  r.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  r.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  r.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  r.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  r.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  r.isDeleted = #{condition.isDeleted}</if><if test="condition.code != null" >and  r.code = #{condition.code}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and r.id = #{condition.id}	</if>													 			<if test="condition.orderID != null" >and  UPPER(r.orderID) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.orderID}),'%'))</if><if test="condition.refundFee != null" >and  UPPER(r.refundFee) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.refundFee}),'%'))</if><if test="condition.reason != null" >and  UPPER(r.reason) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.reason}),'%'))</if><if test="condition.createBy != null" >and  UPPER(r.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(r.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(r.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(r.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(r.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(r.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if><if test="condition.code != null" >and  UPPER(r.code) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.code}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into refund (id,orderID,refundFee,reason,createBy,createTime,updateBy,updateTime,version,isDeleted,code)  values(#{id,jdbcType=VARCHAR},#{orderID},#{refundFee},#{reason},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted},#{code})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update refund set	<trim  suffixOverrides="," >	<if test="orderID != null ">orderID=#{orderID},</if><if test="refundFee != null ">refundFee=#{refundFee},</if><if test="reason != null ">reason=#{reason},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if><if test="code != null ">code=#{code},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from refund where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Refund"  parameterType="String">	select <include refid="columnList" /> 	from refund  r	where  r.id = #{id}  and r.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from refund  r	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Refund" >	select 	<include refid="columnList"/>	from refund r	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Refund" >	select 	<include refid="columnList"/>	from refund r	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Refund" >	select	<include refid="columnList"/>	from refund r	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Refund" >	select	<include refid="columnList"/>	from refund r	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select r.${idsColumn}	from refund r    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select r.${idsColumn}	from refund r    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Refund" >     select    <include refid="columnList"/>     from refund r    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select>	<!--级联查询-->	<sql id="detailList">		r.id as 'id',		r.version as 'version',r.orderID as 'orderID',r.refundFee as 'refundFee',r.reason as 'reason',r.code as 'code',		wxorder.id as 'wxorder.id',wxorder.prepayID as 'wxorder.prepayID',		wxorder.version as 'wxorder.version',wxorder.Bond as 'wxorder.Bond',wxorder.name as 'wxorder.name',wxorder.description as 'wxorder.description',wxorder.type as 'wxorder.type',wxorder.code as 'wxorder.code',wxorder.status as 'wxorder.status',wxorder.userID as 'wxorder.userID',wxorder.fee as 'wxorder.fee'	</sql>	<!--Wxorder映射-->	<resultMap id="WxorderMap" type="Wxorder">		<id property="id" column="wxorder.id" />		<result property="version" column="wxorder.version" />		<result property="Bond" column="wxorder.Bond" />		<result property="name" column="wxorder.name" />		<result property="description" column="wxorder.description" />		<result property="type" column="wxorder.type" />		<result property="code" column="wxorder.code" />		<result property="status" column="wxorder.status" />		<result property="userID" column="wxorder.userID" />		<result property="fee" column="wxorder.fee" />		<result property="prepayID" column="wxorder.prepayID" />	</resultMap>	<!--Refund映射-->	<resultMap id="RefundMap" type="Refund">		<id property="id" column="id" />		<result property="version" column="version" />		<result property="orderID" column="orderID" />		<result property="refundFee" column="refundFee" />		<result property="reason" column="reason" />		<result property="code" column="code" />		<association property="wxorder"  javaType="Wxorder" resultMap="WxorderMap"/>	</resultMap>	<!--对象的多级映射 mapList mapPage mapOne-->	<select id="mapOne" resultMap="RefundMap" >		select		<include refid="detailList"/>		from  refund r		left join wxorder wxorder on r.orderID = wxorder.id		<include refid="whereClause"/>		<if test="orderByClause!=null">${orderByClause}</if>	</select></mapper>