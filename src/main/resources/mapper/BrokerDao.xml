<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.BrokerDao" ><!-- 查询字段 --><sql id="columnList" >	pb.id,	 pb.code, pb.name, pb.level, pb.createBy, pb.createTime, pb.updateBy, pb.updateTime, pb.version, pb.isDeleted, pb.status</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  pb.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and pb.id = #{condition.id}	</if>	<if test="condition.code != null" >and  pb.code = #{condition.code}</if><if test="condition.name != null" >and  pb.name = #{condition.name}</if><if test="condition.level != null" >and  pb.level = #{condition.level}</if><if test="condition.createBy != null" >and  pb.createBy = #{condition.createBy}</if><if test="condition.createTime != null" >and  pb.createTime = #{condition.createTime}</if><if test="condition.updateBy != null" >and  pb.updateBy = #{condition.updateBy}</if><if test="condition.updateTime != null" >and  pb.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  pb.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  pb.isDeleted = #{condition.isDeleted}</if><if test="condition.status != null" >and  pb.status = #{condition.status}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and pb.id = #{condition.id}	</if>													 			<if test="condition.code != null" >and  UPPER(pb.code) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.code}),'%'))</if><if test="condition.name != null" >and  UPPER(pb.name) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.name}),'%'))</if><if test="condition.level != null" >and  UPPER(pb.level) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.level}),'%'))</if><if test="condition.createBy != null" >and  UPPER(pb.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.createTime != null" >and  UPPER(pb.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(pb.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(pb.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(pb.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(pb.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if><if test="condition.status != null" >and  UPPER(pb.status) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.status}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into post_broker (id,code,name,level,createBy,createTime,updateBy,updateTime,version,isDeleted,status)  values(#{id,jdbcType=VARCHAR},#{code},#{name},#{level},#{createBy},#{createTime},#{updateBy},#{updateTime},#{version},#{isDeleted},#{status})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update post_broker set	<trim  suffixOverrides="," >	<if test="code != null ">code=#{code},</if><if test="name != null ">name=#{name},</if><if test="level != null ">level=#{level},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateBy != null ">updateBy=#{updateBy},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if><if test="status != null ">status=#{status},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from post_broker where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Broker"  parameterType="String">	select <include refid="columnList" /> 	from post_broker  pb	where  pb.id = #{id}  and pb.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from post_broker  pb	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Broker" >	select 	<include refid="columnList"/>	from post_broker pb	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Broker" >	select 	<include refid="columnList"/>	from post_broker pb	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Broker" >	select	<include refid="columnList"/>	from post_broker pb	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Broker" >	select	<include refid="columnList"/>	from post_broker pb	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select pb.${idsColumn}	from post_broker pb    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select pb.${idsColumn}	from post_broker pb    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Broker" >     select    <include refid="columnList"/>     from post_broker pb    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>