<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.yskj.dao.HuntingtypeDao" ><!-- 查询字段 --><sql id="columnList" >	h.id,	 h.name, h.HTFatCode, h.state, h.codeGrade, h.createTime, h.updateTime, h.version, h.isDeleted, h.createBy, h.updateBy</sql><!--通过id集合in查询--><sql id="inClause">    <if test="column!=null">        <if test="ids==null">            and 1=0        </if>        <if test="ids!=null">            <if test="ids.size>0">                and  h.${column} in                <foreach item="items" index="index" collection="ids" open="(" separator="," close=")">                    #{items}                </foreach>            </if>            <if test="ids.size==0">                and 1=0            </if>        </if>    </if></sql><!-- 查询条件 --><sql id="whereClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and h.id = #{condition.id}	</if>	<if test="condition.name != null" >and  h.name = #{condition.name}</if><if test="condition.HTFatCode != null" >and  h.HTFatCode = #{condition.HTFatCode}</if><if test="condition.state != null" >and  h.state = #{condition.state}</if><if test="condition.codeGrade != null" >and  h.codeGrade = #{condition.codeGrade}</if><if test="condition.createTime != null" >and  h.createTime = #{condition.createTime}</if><if test="condition.updateTime != null" >and  h.updateTime = #{condition.updateTime}</if><if test="condition.version != null" >and  h.version = #{condition.version}</if><if test="condition.isDeleted != null" >and  h.isDeleted = #{condition.isDeleted}</if><if test="condition.createBy != null" >and  h.createBy = #{condition.createBy}</if><if test="condition.updateBy != null" >and  h.updateBy = #{condition.updateBy}</if></trim><include refid="inClause"/></sql><!-- 模糊查询条件 --><sql id="whereLikeClause">where 1=1<trim  suffixOverrides="," >	<if test="condition.id != null" >	    and h.id = #{condition.id}	</if>													 			<if test="condition.name != null" >and  UPPER(h.name) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.name}),'%'))</if><if test="condition.HTFatCode != null" >and  UPPER(h.HTFatCode) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.HTFatCode}),'%'))</if><if test="condition.state != null" >and  UPPER(h.state) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.state}),'%'))</if><if test="condition.codeGrade != null" >and  UPPER(h.codeGrade) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.codeGrade}),'%'))</if><if test="condition.createTime != null" >and  UPPER(h.createTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createTime}),'%'))</if><if test="condition.updateTime != null" >and  UPPER(h.updateTime) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateTime}),'%'))</if><if test="condition.version != null" >and  UPPER(h.version) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.version}),'%'))</if><if test="condition.isDeleted != null" >and  UPPER(h.isDeleted) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.isDeleted}),'%'))</if><if test="condition.createBy != null" >and  UPPER(h.createBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.createBy}),'%'))</if><if test="condition.updateBy != null" >and  UPPER(h.updateBy) LIKE   UPPER(CONCAT(CONCAT('%',#{condition.updateBy}),'%'))</if></trim><include refid="inClause"/></sql><!-- 插入记录 --><insert id="add"  >  insert into huntingtype (id,name,HTFatCode,state,codeGrade,createTime,updateTime,version,isDeleted,createBy,updateBy)  values(#{id,jdbcType=VARCHAR},#{name},#{HTFatCode},#{state},#{codeGrade},#{createTime},#{updateTime},#{version},#{isDeleted},#{createBy},#{updateBy})</insert><!-- 修改记录，只修改只不为空的字段 --><update id="update" >	update huntingtype set	<trim  suffixOverrides="," >	<if test="name != null ">name=#{name},</if><if test="HTFatCode != null ">HTFatCode=#{HTFatCode},</if><if test="state != null ">state=#{state},</if><if test="codeGrade != null ">codeGrade=#{codeGrade},</if><if test="createTime != null ">createTime=#{createTime},</if><if test="updateTime != null ">updateTime=#{updateTime},</if><if test="version != null ">version=#{version},</if><if test="isDeleted != null ">isDeleted=#{isDeleted},</if><if test="createBy != null ">createBy=#{createBy},</if><if test="updateBy != null ">updateBy=#{updateBy},</if>	</trim> where id=#{id} and version = #{version}-1</update><!-- 删除记录 --><delete id="del" >	delete from huntingtype where id = #{id}</delete><!-- 根据id查询  --><select id="get"   resultType="Huntingtype"  parameterType="String">	select <include refid="columnList" /> 	from huntingtype  h	where  h.id = #{id}  and h.isDeleted = false</select><!-- 列表总数--><select id="count" resultType="java.lang.Long" >	select count(1) 	from huntingtype  h	<include refid="whereClause"/></select><!-- 查询page --><select id="findPage" resultType="Huntingtype" >	select 	<include refid="columnList"/>	from huntingtype h	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 模糊查询page --><select id="findLikePage" resultType="Huntingtype" >	select 	<include refid="columnList"/>	from huntingtype h	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询列表list --><select id="findList" resultType="Huntingtype" >	select	<include refid="columnList"/>	from huntingtype h	<include refid="whereClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!--模糊查询list--><select id="findLikeList" resultType="Huntingtype" >	select	<include refid="columnList"/>	from huntingtype h	<include refid="whereLikeClause"/>	<if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合条件的数据的id集合 --><select id="findIds" resultType="String">	select h.${idsColumn}	from huntingtype h    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 查询符合模糊条件的数据的id集合 --><select id="findLikeIds" resultType="String">	select h.${idsColumn}	from huntingtype h    <include refid="whereLikeClause"/>    <if test="orderByClause!=null">${orderByClause}</if></select><!-- 获取一个对象通过非id --><select id="one" resultType="Huntingtype" >     select    <include refid="columnList"/>     from huntingtype h    <include refid="whereClause"/>    <if test="orderByClause!=null">${orderByClause}</if>    limit 0,1</select><!--如果你想返回一个HashMap 就实现这个select标签--><select id="queryHashMap" resultType="java.util.HashMap"  parameterType="Object"></select></mapper>